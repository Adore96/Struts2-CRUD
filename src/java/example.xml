<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="default" extends="struts-default">
        
        <action name="LoginAction" class="com.Adore96.action.LoginAction" method="Login">
            <result name="success" type="redirect">list.action</result>
            <result name="failure">/Login.jsp</result>
        </action>
        
        <action name="RegisterAction" class="com.Adore96.action.RegisterAction" method="Login">
            <result name="success">/DashBoardAction</result>
            <result name="failure">/Login.jsp</result>
        </action>
        
        <action name="list" class="com.Adore96.action.DashboardAction" method="ListUser">
            <result name="showtabledata">/Dashboard.jsp</result>
            <result name="emptylist">/example/HelloWorld.jsp</result>
        </action>
        
<!--        <action name="*" class="com.Adore96.action.DashBoardAction" method="ListUser">
            <result name="showtabledata" type="redirect">list.action</result>
        </action>-->
        
    </package>
    
</struts>

